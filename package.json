{
  "name": "smartlogic-visualisation",
  "version": "1.3.7",
  "description": "The Ontology Visualizer Component for Semaphore",
  "main": "visualizer-module.js",
  "browser": "visualizer.js",
  "browserify": {
    "transform" :  ["ngify", {
      "moduleName": "visualizer"
    }]
  },
  "dependencies": {
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "ngify": "^1.4.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  },
  "devDependencies": {
    "jsdoc": "^3.4.0",
    "minami": "^1.1.1",
    "plato": "^1.5.0"
  },
  "scripts": {
    "test": "test-visualizer",
    "docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "jshint": "node_modules/.bin/jshint .",
    "plato": "node_modules/.bin/plato -r -d reports -x '(node_modules|reports|docs)' ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AbdoulSy/Visualisation.git"
  },
